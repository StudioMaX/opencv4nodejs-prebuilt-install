{
  "name": "opencv4nodejs-prebuilt-install",
  "version": "4.1.127",
  "description": "Asynchronous OpenCV 4.x nodejs bindings with JavaScript and TypeScript API.",
  "keywords": [
    "opencv",
    "cv",
    "computer vision",
    "face",
    "detection",
    "recognition",
    "machine",
    "learning",
    "neural",
    "network",
    "async",
    "typescript"
  ],
  "files": [
    "lib",
    "index.js"
  ],
  "author": {
    "name": "Simon Hofmann",
    "email": "kontak@s1h.org",
    "url": "https://s1h.org"
  },
  "contributors": [
    "Siarhei Kliushnikau <sergey.klyshnikov2@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udarrr/opencv4nodejs-prebuilt"
  },
  "bugs": {
    "url": "https://github.com/udarrr/opencv4nodejs-prebuilt/issues"
  },
  "homepage": "https://github.com/udarrr/opencv4nodejs-prebuilt/#readme",
  "main": "./lib/opencv4nodejs.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "install": "prebuild-install || node ./index.js || exit 0 ",
    "clean": "node-gyp clean",
    "configure": "node-gyp configure",
    "build": "node ./install/dependencies.js && node-gyp build --jobs max",
    "build-debug": "BINDINGS_DEBUG=true npm run build",
    "rebuild": "node ./install/dependencies.js && node-gyp rebuild --jobs max",
    "prebuild": "node ./install/dependencies.js && prebuild --include-regex \"\\.(node|a|so|dylib|lib|dll).*$\"",
    "prepack": "clean-package --remove scripts",
    "postpack": "clean-package restore",
    "pack": "npm pack",
    "compile": "",
    "prepublishOnly": "npm run compile",
    "publish": "npm publish --tag latest --access=public"
  },
  "gypfile": false,
  "dependencies": {
    "nan": "^2.17.0",
    "native-node-utils": "^0.2.7",
    "npmlog": "^7.0.1",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "prebuild": "^11.0.4"
  },
  "optionalDependencies": {
    "@types/node": ">12"
  },
  "opencv4nodejs": {
    "disableAutoBuild": 1
  },
  "config": {
    "opencv": "4.1.1-7",
    "ci": true
  },
  "engines": {
    "node" : ">=12.0.0 <19.0.0"
  }
}
